{
  "presets": [
    "es2015",
    "stage-0",
    [
      "env",
      {
        "targets": {
          "browsers": "last 2 versions"
        },
        "useBuiltIns": true,
        "debug": false
      }
    ]
  ],
  "plugins": [
    "transform-runtime",
    "transform-regenerator"
  ],
  "ignore": [
    "node_modules"
  ]
}